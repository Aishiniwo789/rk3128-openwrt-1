CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_generic=y
CONFIG_TARGET_rockchip_generic_DEVICE_rk3128-box=y
CONFIG_TARGET_BOARD="rockchip"
CONFIG_TARGET_SUBTARGET="generic"
CONFIG_TARGET_PROFILE="DEVICE_rk3128-box"
CONFIG_TARGET_ARCH_PACKAGES="arm_cortex-a7_neon-vfpv4"
CONFIG_ARCH="arm"
CONFIG_ARCH_PACKAGES="arm_cortex-a7_neon-vfpv4"
# 固件打包核心开关 - 必开，无此项无固件，绝对不删
CONFIG_TARGET_ROOTFS=y
CONFIG_TARGET_ROOTFS_SQUASHFS=y
CONFIG_TARGET_ROOTFS_SQUASHFS_BLOCK_SIZE=256
CONFIG_TARGET_ROOTFS_EXT4FS=y
CONFIG_TARGET_ROOTFS_PARTSIZE=300
CONFIG_TARGET_IMAGES=y
CONFIG_TARGET_IMAGES_GZIP=y
# 永久禁用64位，杜绝NanoPi R2S错误固件，必加
CONFIG_TARGET_rockchip_armv8=n
# 中文Luci全套组件 - 纯净无冗余，中文界面
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-i18n-base-zh-cn=y
CONFIG_PACKAGE_luci-theme-bootstrap=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-app-firewall=y
CONFIG_PACKAGE_luci-app-opkg=y
# 网络核心组件 - WiFi+有线双兼容，无线打印必备
CONFIG_PACKAGE_dnsmasq-full=y
CONFIG_PACKAGE_odhcp6c=y
CONFIG_PACKAGE_odhcpd-ipv6only=y
CONFIG_PACKAGE_ipv6helper=y
CONFIG_DEFAULT_ppp=y
CONFIG_DEFAULT_ppp-mod-pppoe=y
# WiFi完整驱动 - 修复冲突，完美编译，RK3128稳定运行
CONFIG_PACKAGE_kmod-wireless=y
CONFIG_PACKAGE_kmod-brcmfmac=y
CONFIG_PACKAGE_kmod-brcmutil=y
CONFIG_PACKAGE_wpad-openssl=y
CONFIG_PACKAGE_wireless-tools=y
CONFIG_PACKAGE_iw=y
# USB核心驱动 + 打印机专用驱动 - 打印服务重中之重，全部保留
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-usb-printer=y
CONFIG_PACKAGE_usbutils=y
CONFIG_KERNEL_USB_PRINTER=y
CONFIG_PACKAGE_usbmode=y
# CUPS完整打印服务 + 无线打印协议 - 手机无线打印核心，全部保留
CONFIG_PACKAGE_cups=y
CONFIG_PACKAGE_cups-filters=y
CONFIG_PACKAGE_cups-libs=y
CONFIG_PACKAGE_cups-bjnp=y
CONFIG_PACKAGE_avahi-daemon=y
CONFIG_PACKAGE_avahi-autoipd=y
CONFIG_PACKAGE_luci-app-cups=y
# 基础工具 - 够用即可，无冗余，不增加编译负担
CONFIG_PACKAGE_iputils-ping=y
CONFIG_PACKAGE_net-tools=y
CONFIG_PACKAGE_wget=y
# RK3128硬件专属适配 - 只保留稳定项，消除内核冲突
CONFIG_KERNEL_ARM_APPENDED_DTB=y
CONFIG_KERNEL_ARM_ATAG_DTB_COMPAT=y
CONFIG_KERNEL_DEVKMEM=y
CONFIG_KERNEL_DMA_CMA=y
CONFIG_KERNEL_SERIAL_DEV_BUS=y
