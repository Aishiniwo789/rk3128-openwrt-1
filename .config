CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_rk3128=y
CONFIG_TARGET_rockchip_rk3128_DEVICE_generic=y
CONFIG_TARGET_BOARD="rockchip"
CONFIG_TARGET_SUBTARGET="rk3128"
CONFIG_TARGET_PROFILE="DEVICE_generic"
CONFIG_TARGET_ROOTFS_PARTSIZE=300
CONFIG_TARGET_ROOTFS_EXT4FS=y
CONFIG_TARGET_KERNEL_PARTSIZE=64

# USB全驱动（打印机必备）
CONFIG_PACKAGE_kmod-usb-core=y
CONFIG_PACKAGE_kmod-usb-ohci=y
CONFIG_PACKAGE_kmod-usb-uhci=y
CONFIG_PACKAGE_kmod-usb2=y
CONFIG_PACKAGE_kmod-usb-storage=y
CONFIG_PACKAGE_kmod-usb-printer=y
CONFIG_PACKAGE_usbutils=y

# 全套CUPS打印服务（内置）
CONFIG_PACKAGE_cups=y
CONFIG_PACKAGE_cups-filters=y
CONFIG_PACKAGE_cups-libs=y
CONFIG_PACKAGE_ghostscript=y
CONFIG_PACKAGE_gsfonts=y
CONFIG_PACKAGE_avahi-daemon=y
CONFIG_PACKAGE_avahi-utils=y

# 可视化管理+中文
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-app-cups=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-i18n-base-zh-cn=y
CONFIG_PACKAGE_luci-theme-bootstrap=y
CONFIG_PACKAGE_luci-mod-admin-full=y

# 【新增】WiFi驱动+管理（适配AP6212/AP6210）
CONFIG_PACKAGE_kmod-brcmfmac=y
CONFIG_PACKAGE_kmod-brcmutil=y
CONFIG_PACKAGE_kmod-mac80211=y
CONFIG_PACKAGE_kmod-cfg80211=y
CONFIG_PACKAGE_wireless-tools=y
CONFIG_PACKAGE_wpa_supplicant=y
CONFIG_PACKAGE_hostapd=y
CONFIG_PACKAGE_luci-app-wireless=y
CONFIG_PACKAGE_luci-i18n-wireless-zh-cn=y

# 网口+基础工具
CONFIG_PACKAGE_kmod-nls-cp437=y
CONFIG_PACKAGE_kmod-nls-utf8=y
CONFIG_PACKAGE_iputils-ping=y
CONFIG_PACKAGE_wget=y
CONFIG_PACKAGE_net-tools=y

# 内核优化
CONFIG_KERNEL_PRINTK=y
CONFIG_KERNEL_DEVMEM=y
CONFIG_KERNEL_USB_PRINTER=y
CONFIG_KERNEL_NET_RMII=y
CONFIG_KERNEL_WLAN=y
